@model CustomerProfileModel
@{
    CustomerProfileModel bindmodel = ViewBag.Profile;
}
<div class="breadcrumb-bar">
    <div class="container-fluid">
        <div class="row align-items-center">
            <div class="col-md-12 col-12">
                <nav aria-label="breadcrumb" class="page-breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Profile Settings</li>
                    </ol>
                </nav>
                <h2 class="breadcrumb-title">Profile Settings</h2>
            </div>
        </div>
    </div>
</div>


<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-7 col-lg-8 col-xl-9">
                <div class="card">
                    <div class="card-body">

                        <form method="post" asp-action="UpdateProfile">
                            <div class="row form-row">
                                <div class="col-12 col-md-6">
                                    <div class="form-group">
                                        <label>Tên đầy đủ</label>
                                        <input type="text" class="form-control" value="@bindmodel.FullName" asp-for="FullName">
                                    </div>
                                </div>
                                <div class="col-12 col-md-6">
                                    <div class="form-group">
                                        <label>Ngày sinh</label>
                                        <div class="cal-icon">
                                            <input type="text" class="form-control datetimepicker" asp-for="DateOfBirth"
                                                   value="@bindmodel.DateOfBirth">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-md-6">
                                    <div class="form-group">
                                        <label>Giới tính</label>
                                        <select class="form-control select" asp-for="Gender">
                                            <option>Nam</option>
                                            <option>Nữ</option>
                                            
                                        </select>
                                    </div>
                                </div>                             
                            </div>
                            <div class="submit-section">
                                <button type="submit" class="btn btn-primary submit-btn">Cập nhật</button>
                            </div>
                        </form>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


